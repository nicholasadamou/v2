{
	"name": "nicholasadamou.com",
	"version": "1.0.0",
	"private": true,
	"description": "Nicholas Adamou's Personal Website",
	"author": "Nicholas Adamou <nicholasadamouemail@gmail.com>",
	"repository": {
		"type": "git",
		"url": "https://github.com/nicholadamou/nicholasadamou.com"
	},
	"keywords": [
		"gatsby"
	],
	"engines": {
		"node": ">=0.12"
	},
	"scripts": {
		"develop": "gatsby develop --host 0.0.0.0 --port 8000",
		"start": "npm run develop",
		"build": "gatsby build",
		"serve": "gatsby serve --host 0.0.0.0",
		"clean": "gatsby clean",
		"format": "prettier --write \"**/*.{js,jsx,json,md}\"",
		"prepare": "husky install",
		"lint-staged": "lint-staged",
		"update": "npx yarn-check -u"
	},
	"lint-staged": {
		"*.{js,css,json,md}": [
			"prettier --write"
		],
		"*.js": [
			"eslint --fix"
		]
	},
	"dependencies": {
		"@fortawesome/fontawesome-svg-core": "1.2.35",
		"@fortawesome/free-brands-svg-icons": "5.15.3",
		"@fortawesome/free-regular-svg-icons": "5.15.3",
		"@fortawesome/free-solid-svg-icons": "5.15.3",
		"@fortawesome/react-fontawesome": "0.1.14",
		"@material-ui/core": "4.12.2",
		"@nicholasadamou/react-masonry": "^0.1.4",
		"@reach/router": "^1.3.4",
		"babel-plugin-styled-components": "1.13.2",
		"browser-monads": "^1.0.0",
		"bulma": "0.9.3",
		"carbon-components": "10.40.0",
		"carbon-components-react": "7.40.0",
		"carbon-icons": "7.0.7",
		"dotenv": "10.0.0",
		"gatsby": "3.10.1",
		"gatsby-alias-imports": "^1.0.6",
		"gatsby-image": "3.10.0",
		"gatsby-plugin-google-analytics": "3.10.0",
		"gatsby-plugin-image": "1.10.1",
		"gatsby-plugin-netlify": "3.10.0",
		"gatsby-plugin-react-helmet": "4.10.0",
		"gatsby-plugin-react-svg": "^3.0.1",
		"gatsby-plugin-robots-txt": "^1.6.8",
		"gatsby-plugin-sass": "4.10.0",
		"gatsby-plugin-sharp": "3.10.2",
		"gatsby-plugin-sitemap": "^4.6.0",
		"gatsby-plugin-styled-components": "4.10.0",
		"gatsby-remark-external-links": "^0.0.4",
		"gatsby-source-filesystem": "3.10.0",
		"gatsby-source-graphql": "3.10.0",
		"gatsby-transformer-remark": "4.7.0",
		"gatsby-transformer-sharp": "3.10.0",
		"moment": "2.29.1",
		"node-sass": "5.0.0",
		"react": "17.0.2",
		"react-device-detect": "^1.17.0",
		"react-dom": "17.0.2",
		"react-helmet": "^6.1.0",
		"styled-components": "5.3.0"
	},
	"devDependencies": {
		"@babel/core": "^7.14.0",
		"@babel/eslint-parser": "^7.13.14",
		"@babel/preset-react": "^7.13.13",
		"@upstatement/eslint-config": "^1.0.0",
		"@upstatement/prettier-config": "^1.0.0",
		"babel-preset-gatsby": "^1.4.0",
		"eslint": "^7.25.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-jsx-a11y": "^6.4.1",
		"eslint-plugin-react": "^7.23.2",
